@page
@model Employee_CRUD.Pages.WorkHour.IndexWorkModel
@{
}
<br />
<h2>
    Employee Work Hour Details @Model.workInfo.id
</h2>

<div class="row mb-3">
    <label class="col-sm-3 col-form-label">Id</label>
    <div class="col-sm-6">
        <input type="text" class="form-control" name="id" value="" />
    </div>
</div>

<div class="row mb-3">
    <label class="col-sm-3 col-form-label">Company Working Hour</label>
    <div class="col-sm-6">
        <input type="text" class="form-control" name="desig" value="@Model.workInfo.company_hr" />
    </div>
</div>

<div class="row mb-3">
    <label class="col-sm-3 col-form-label">Employee Working</label>
    <div class="col-sm-6">
        <input type="text" class="form-control" name="phone" value="@Model.workInfo.emp_hr" />
    </div>
</div>